<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minWidth="25px"
    android:minHeight="25px"
    android:background="#ff455a64">
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linearLayout1"
        android:background="#ff9e9e9e"
        android:layout_margin="25px">
        <TextView
            android:text="Show Page"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="59.0dp"
            android:id="@+id/textView1"
            android:padding="10px"
            android:background="#ff2d2828"
            android:layout_margin="20px"
            android:gravity="center"
            android:textColor="#fff7f7fa"
            android:textSize="30dp" />
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="176.5dp"
            android:id="@+id/linearLayout3"
            android:gravity="center"
            android:background="#ff006064"
            android:layout_margin="20px">
            <EditText
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="58.0dp"
                android:id="@+id/username"
                android:padding="10px"
                android:background="#fff7f0f0"
                android:layout_margin="15dp"
                android:gravity="center"
                android:textColor="#ff1a237e"
                android:textSize="30dp"
                android:hint="UserName" />

            <EditText
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="58.0dp"
                android:id="@+id/password"
                android:padding="10px"
                android:background="#fff7f0f0"
                android:layout_margin="15dp"
                android:gravity="center"
                android:textColor="#ff1a237e"
                android:textSize="30dp"
                android:hint="Password" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout2"
            android:gravity="center"
            android:background="#ff757575"
            android:layout_margin="20px">
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="92.5dp"
                android:id="@id/linearLayout4"
                android:gravity="center"
                android:background="#ff424242"
                android:layout_margin="20px">
                <Button
                    android:text="Call"
                    android:layout_width="wrap_content"
                    android:layout_height="72.0dp"
                    android:id="@+id/call"
                    android:layout_margin="10px"
                    android:padding="10px"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    app:backgroundTint="#ff455a64" />
                <Button
                    android:text="Map"
                    android:layout_width="wrap_content"
                    android:layout_height="72.0dp"
                    android:id="@+id/map"
                    android:layout_margin="10px"
                    android:padding="10px"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    app:backgroundTint="#ff455a64"/>
                <Button
                    android:text="Clear"
                    android:layout_width="wrap_content"
                    android:layout_height="72.0dp"
                    android:id="@+id/clear"
                    android:layout_margin="10px"
                    android:padding="10px"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    app:backgroundTint="#ff455a64"/>
            </LinearLayout>
        </LinearLayout>
         <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout5"
            android:gravity="center"
            android:background="#ff757575"
            android:layout_margin="20px">
            <Button
                android:text="Web"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:id="@+id/web"
                android:layout_margin="10px"
                android:padding="10px"
                android:textStyle="bold"
                android:textSize="20dp"
                app:backgroundTint="#ff455a64" />
            <Button
                android:text="Next"
                android:layout_width="wrap_content"
                android:layout_height="72.0dp"
                android:id="@+id/next"
                android:layout_margin="10px"
                android:padding="10px"
                android:textStyle="bold"
                android:textSize="20dp"
                app:backgroundTint="#ff455a64"/>
           
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>